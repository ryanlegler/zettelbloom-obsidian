		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"chokidar": "^3.6.0",
		"concurrently": "^8.2.2",
		"dotenv": "^16.4.5",
		"fs-extra": "^11.2.0"
	}
